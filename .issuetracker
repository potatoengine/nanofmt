# Integration with Issue Tracker
#
# (note that '\' need to be escaped).

[issuetracker "GitHub PRs"]
  regex = "\\s\\(#(\\d+)\\)$"
  url = "https://github.com/potatoengine/nanofmt/pull/$1"

[issuetracker "GitHub Issues"]
  regex = "#(\\d+)"
  url = "https://github.com/potatoengine/nanofmt/issues/$1"
